{
  "name": "vite-ts-mobx-react",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git"
  },
  "scripts": {
    "dev": "vite --mode development",
    "start": "vite --mode development",
    "build:preview": "vite build --mode preview",
    "build:release": "vite build --mode release",
    "preinstall": "npx only-allow yarn",
    "postinstall": "yarn husky install",
    "serve": "vite preview",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix"
  },
  "dependencies": {
    "@ant-design/icons": "^4.5.0",
    "animate.css": "^4.1.1",
    "antd": "4.16.13",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "dayjs": "^1.10.4",
    "lodash-es": "^4.17.21",
    "mobx": "^6.1.8",
    "mobx-react-lite": "^3.2.0",
    "query-string": "^6.14.1",
    "react": "^17.0.1",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^17.0.1",
    "react-router-cache-route": "^1.11.1",
    "react-router-dom": "^5.2.0",
    "reflect-metadata": "^0.1.13",
    "stylelint-config-recess-order": "^2.5.0",
    "tailwindcss": "^2.0.3",
    "tsyringe": "^4.4.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/preset-typescript": "^7.13.0",
    "@types/classnames": "^2.2.11",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^14.14.34",
    "@types/react": "^17.0.1",
    "@types/react-addons-css-transition-group": "^15.0.7",
    "@types/react-dom": "^17.0.1",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "@vitejs/plugin-legacy": "^1.3.1",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "autoprefixer": "^10.2.4",
    "babel-plugin-transform-typescript-metadata": "^0.3.1",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.1.3",
    "less": "^4.1.1",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^10.5.4",
    "postcss": "^8.2.8",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "rimraf": "^3.0.2",
    "stylelint": "^13.12.0",
    "stylelint-config-recommended": "^4.0.0",
    "stylelint-order": "^4.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2",
    "vite": "^2.0.5",
    "vite-plugin-babel-import": "^2.0.2",
    "vite-plugin-imp": "^2.0.4",
    "vite-plugin-svg-icons": "^1.0.5"
  },
  "engines": {
    "node": "^12 || >=14"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "pretty-quick --staged",
      "eslint --fix --quiet",
      "git add"
    ],
    "*.{css,scss,less}": [
      "stylelint"
    ]
  },
  "description": "This template augments npm init @vitejs/app app -- --template react-ts with: a vite+react+mobx startkit",
  "bugs": {
    "url": "https://github.com/wgfire/React-vite-mobx-ts/issues"
  },
  "homepage": "https://github.com/wgfire/React-vite-mobx-ts",
  "main": "src",
  "keywords": [
    "vite"
  ]
}
